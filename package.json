{
  "name": "copy-all-files-vscode-extension",
  "displayName": "Copy All Files",
  "description": "Copy all files from workspace to clipboard with custom format",
  "version": "0.1.6",
  "publisher": "brianuceda",
  "license": "MIT",
  "icon": "images/copy-all-files-vscode-extension-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/brianuceda/copy-all-files-vscode-extension"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-all-files.copyFiles",
        "title": "Copy All Files to Clipboard"
      },
      {
        "command": "copy-all-files.copyTree",
        "title": "Copy Project Structure (Tree) to Clipboard"
      }
    ]
  }
}
